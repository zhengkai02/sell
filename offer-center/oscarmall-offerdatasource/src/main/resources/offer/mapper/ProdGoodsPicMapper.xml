<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.iwhalecloud.retail.offer.mapper.ProdGoodsPicMapper">

    <select id="qryPicUrl" resultType="string">
        select pic_url
          from tbl_prod_goods_pic
         where goods_Id = #{goodsId}
    </select>

</mapper>